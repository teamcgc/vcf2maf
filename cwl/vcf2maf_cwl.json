{
  "sbg:id": "vcf2maf",
  "id": "#vcf2maf",
  "inputs": [
    {
      "type": ["null", "File"],
      "label": "vcf",
      "description": "vcf",
      "streamable": false,
      "default": "",
      "id": "#vcf",
      "required": false
    }
  ],
  "outputs": [
    {
      "type": ["null", "File"],
      "label": "maf",
      "description": "maf",
      "streamable": false,
      "default": "",
      "id": "#maf",
      "outputBinding": {
        "glob": "\"*.maf\"",
        "sbg:metadata": {
          "org": "cgp"
        }
      }
    },
    {
      "type": [
        "null",
        {
          "items": "File",
          "type": "array"
        }
      ],
      "label": "std_out",
      "description": "standard output",
      "streamable": false,
      "default": "",
      "id": "#std_out",
      "outputBinding": {
        "glob": "\"std.out\"",
        "sbg:metadata": {
          "org": "cgp"
        }
      }
    }
  ],
  "requirements": [],
  "hints": [
    {
      "class": "DockerRequirement",
      "dockerPull": "teamcgc/vcf2maf:latet"
    },
    {
      "class": "sbg:CPURequirement",
      "value": 1
    },
    {
      "class": "sbg:MemRequirement",
      "value": 1000
    }
  ],
  "label": "vcf2maf",
  "class": "CommandLineTool",
  "baseCommand": [
    "perl vcf2maf.pl --input-vcf data/test.vcf --output-maf test.vep.maf\n"
